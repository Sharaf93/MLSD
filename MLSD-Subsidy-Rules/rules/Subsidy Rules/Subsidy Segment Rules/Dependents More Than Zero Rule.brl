<?xml version="1.0" encoding="UTF-8"?>
<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>Dependents More Than Zero Rule</name>
  <uuid>34c243a0-b407-411a-9abf-40bc7acbfd83</uuid>
  <locale>en_US</locale>
  <definition><![CDATA[if
    the number of elements in the eligible dependents of 'the applicant' equals 0
then
    set the eligibility of 'the applicant' to Ineligible ;
    set the rejection message of 'the applicant' to MSG-FS1 ;]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
