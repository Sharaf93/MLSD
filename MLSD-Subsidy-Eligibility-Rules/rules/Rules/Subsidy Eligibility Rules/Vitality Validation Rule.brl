<?xml version="1.0" encoding="UTF-8"?>
<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>Vitality Validation Rule</name>
  <uuid>3cc00cab-3c8c-4f4a-a4b1-07fabd497cc3</uuid>
  <locale>en_US</locale>
  <definition><![CDATA[if
    all of the following conditions are true :
            - the vitality status of 'the applicant' is deceased
            - ( 'the applicant' has no ( ALIVE ) appeal
            or it is not true that 'the applicant' ( ALIVE ) appeal status is approved ) ,
then
    set the eligibility of 'the applicant' to Ineligible ;
    set the rejection message of 'the applicant' to MSG-B2 ;]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
