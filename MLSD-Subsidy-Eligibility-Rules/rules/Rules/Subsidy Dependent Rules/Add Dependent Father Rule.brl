<?xml version="1.0" encoding="UTF-8"?>
<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>Add Dependent Father Rule</name>
  <uuid>a77260a0-5152-475c-bd21-8ba6f8ebdc8a</uuid>
  <locale>en_US</locale>
  <definition><![CDATA[if
    the guardian of the father of 'the applicant' is 'the applicant'
then
    add the father of 'the applicant' to eligible dependents of 'the applicant' ;
else
    set the rejection message of the father of 'the applicant' to MSG-D5 ;
    add the father of 'the applicant' to ineligible dependents of 'the applicant' ;]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
